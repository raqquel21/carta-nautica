# Reemplaza tu .gitignore con este contenido
cat > .gitignore << 'EOF'
# Directorios de build
[bB]uild*/
[Bb]uild-*/
*/[bB]uild*/
*/[bB]uild-*/
*.[Dd]ebug/
*.[Rr]elease/
*_build/
*.xcodeproj/
*.xcworkspace/

# Qt
.qtc_clangd/
*.user
*.user.*
*.autosave
moc_*.cpp
moc_*.h
qrc_*.cpp
ui_*.h
Makefile*
*.pro.user
*.qmake.cache
*.qmake.stash
*.qm

# Objetos C++
*.o
*.obj
*.a
*.lib
*.so
*.so.*
*.dll
*.dylib

# Archivos generados por CMake
CMakeCache.txt
CMakeFiles/
cmake_install.cmake
Makefile
CMakeLists.txt.user*

# Archivos de IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Archivos de debug
core
*.core
*.stackdump

# QML
*.qmlc
*.jsc

# Unit tests
target_wrapper.*
EOF
